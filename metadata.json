{
  "name": "ccin2p3-riemann",
  "version": "1.2.0",
  "author": "ccin2p3",
  "summary": "Manage the riemann streams processor and its dashboard",
  "license": "CECILL-B",
  "source": "https://github.com/ccin2p3/puppet-riemann",
  "project_page": "https://github.com/ccin2p3/puppet-riemann",
  "issues_url": "https://github.com/ccin2p3/puppet-riemann/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": "> 4.4.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": "> 1.1.1 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8.0",
        "9.0",
        "10.0"
      ]
    },
    {
      "operatingsystem": "FreeBSD"
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0",
        "8.0"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.9.0 < 7.0.0"
    }
  ]
}
