      <%=@async_queue_name%>
        (batch <%=@batch%>
          (exception-stream
            (throttle <%=@throttle%>
              (with {:host our-host :service "<%=@async_queue_name%>" :state "warning"} (adjust [:event #(subs % 0 (min 32 (count %)))] #(info %))))
            (async-queue! :<%=@async_queue_name%> {<%=@async_queue_options_string%>}
              (forward (tcp-client :host "<%=@clientcert%>")))))
